{
  "name": "CSS HEAD INSPECTOR",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension allows you to inspect hidden html elements.",
  "homepage_url": "https://canvasplay.github.io/css-head-inspector/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Inspect"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}